{
  "name": "rmd",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "copyfiles -u 1 \"@(src)/**/*.@(json|js)\" lib",
    "build": "tsc --module commonjs --project .",
    "bundle": "webpack",
    "postbundle": "uglifyjs bundle.js -o bundle.min.js --mangle --compress --screw-ie8",
    "lint": "prettier --print-width 100 --trailing-comma all --write \"src/**/*.ts\"",
    "watch": "tsc --module commonjs --project . -w",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"rootDir\":\".\"}' mocha --require ts-node/register test/*",
    "bench": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"rootDir\":\".\"}' ts-node -F -I -D benchmark/index.ts",
    "run": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node -F -I -D src/index.ts"
  },
  "devDependencies": {
    "@types/power-assert": "^1.4.29",
    "awesome-typescript-loader": "^3.2.2",
    "benchmark": "^2.1.4",
    "copyfiles": "^1.2.0",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.2.1",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.27",
    "webpack": "^3.5.3"
  },
  "dependencies": {
    "tslib": "^1.7.1"
  }
}
