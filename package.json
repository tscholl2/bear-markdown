{
  "name": "rmd",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "copyfiles -u 1 \"@(src)/**/*.@(json|js)\" lib",
    "build": "tsc --module commonjs --project .",
    "bundle": "webpack --output-library-target umd lib/index.js dist/bundle.js",
    "postbundle": "webpack -p --output-library-target umd lib/index.js dist/bundle.min.js && gzip -c dist/bundle.min.js > dist/bundle.min.js.gzip",
    "lint": "prettier --print-width 100 --trailing-comma all --write \"src/**/*.ts\"",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"rootDir\":\".\"}' ts-node scripts/test.ts",
    "bench": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"rootDir\":\".\"}' ts-node -F -I -D benchmark/index.ts",
    "run": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"rootDir\":\".\"}' ts-node",
    "presize": "yarn run build",
    "size": "size-limit"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "commonmark": "^0.28.1",
    "copyfiles": "^1.2.0",
    "filehound": "^1.16.2",
    "markdown": "^0.5.0",
    "marked": "^0.3.6",
    "micromarkdown": "^0.3.4-a",
    "prettier": "^1.6.1",
    "showdown": "^1.7.3",
    "simple-markdown": "^0.2.1",
    "size-limit": "^0.10.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.8.0",
    "ts-node": "^3.2.1",
    "tslib": "^1.7.1",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.28",
    "webpack": "^3.5.5"
  },
  "dependencies": {},
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "4 KB"
    }
  ]
}
