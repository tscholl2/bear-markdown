[
  {
    "description": "plain text",
    "input": "something",
    "output": [
      { "tag": "paragraph", "children": [{ "tag": "text", "props": { "content": "something" } }] }
    ]
  },
  {
    "description": "emphasis",
    "input": "this is **something**",
    "output": [
      {
        "tag": "paragraph",
        "children": [
          { "tag": "text", "props": { "content": "this is " } },
          {
            "tag": "emphasis",
            "props": { "delimiter": "**" },
            "children": [{ "tag": "text", "props": { "content": "something" } }]
          }
        ]
      }
    ]
  },
  {
    "description": "list",
    "input": "* A\n* B\n* C",
    "output": [
      {
        "tag": "list",
        "props": { "bullet": "*" },
        "children": [
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "A" } }] },
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "B" } }] },
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "C" } }] }
        ]
      }
    ]
  },
  {
    "description": "emphasis not list",
    "input": "*hello*",
    "output": [
      {
        "tag": "paragraph",
        "children": [
          {
            "tag": "emphasis",
            "props": { "delimiter": "*" },
            "children": [{ "tag": "text", "props": { "content": "hello" } }]
          }
        ]
      }
    ]
  },
  {
    "description": "list not emphasis",
    "input": "* hello*",
    "output": [
      {
        "tag": "list",
        "props": { "bullet": "*" },
        "children": [
          {
            "tag": "listitem",
            "children": [
              { "tag": "text", "props": { "content": "hello" } },
              { "tag": "text", "props": { "content": "*" } }
            ]
          }
        ]
      }
    ]
  },
  {
    "description": "emphasis with escaped delimiter",
    "input": "*\\**",
    "output": [
      {
        "tag": "paragraph",
        "children": [
          {
            "tag": "emphasis",
            "props": { "delimiter": "*" },
            "children": [{ "tag": "text", "props": { "content": "*" } }]
          }
        ]
      }
    ]
  },
  {
    "description": "emphasis with escaped delimiter",
    "input": "**\\****",
    "output": [
      {
        "tag": "paragraph",
        "children": [
          {
            "tag": "emphasis",
            "props": { "delimiter": "**" },
            "children": [
              { "tag": "text", "props": { "content": "*" } },
              { "tag": "text", "props": { "content": "*" } }
            ]
          }
        ]
      }
    ]
  },
  {
    "description": "code block",
    "input": "```this is code```",
    "output": [{ "tag": "code", "props": { "display": "block", "content": "this is code" } }]
  },
  {
    "description": "code block with escaped ```",
    "input": "```code\\```code```",
    "output": [{ "tag": "code", "props": { "display": "block", "content": "code```code" } }]
  },
  {
    "description": "code block with escaped \\```",
    "input": "```code\\\\```code```",
    "output": [{ "tag": "code", "props": { "display": "block", "content": "code\\```code" } }]
  },
  {
    "description": "lists with 2 items",
    "input": "* A\n* B",
    "output": [
      {
        "tag": "list",
        "props": { "bullet": "*" },
        "children": [
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "A" } }] },
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "B" } }] }
        ]
      }
    ]
  },
  {
    "description": "lists with 2 bullets",
    "input": "* A\n- B",
    "output": [
      {
        "tag": "list",
        "props": { "bullet": "*" },
        "children": [
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "A" } }] }
        ]
      },
      {
        "tag": "list",
        "props": { "bullet": "-" },
        "children": [
          { "tag": "listitem", "children": [{ "tag": "text", "props": { "content": "B" } }] }
        ]
      }
    ]
  },
  {
    "description": "image",
    "input": "![alt](src \"title\")",
    "output": [
      {
        "tag": "paragraph",
        "children": [{ "tag": "image", "props": { "alt": "alt", "title": "title", "src": "src" } }]
      }
    ]
  },
  {
    "description": "math",
    "input": "$1+1$",
    "output": [
      {
        "tag": "paragraph",
        "children": [{ "tag": "math", "props": { "display": "inline", "content": "1+1" } }]
      }
    ]
  },
  {
    "description": "math",
    "input": "$$1+1$$",
    "output": [{ "tag": "math", "props": { "display": "block", "content": "1+1" } }]
  },
  {
    "description": "basic table",
    "input": "| A | B |\n| - | - |\n| c | d |",
    "output": [
      {
        "tag": "table",
        "children": [
          {
            "tag": "tablehead",
            "children": [
              {
                "tag": "tableheadcolumn",
                "props": { "align": "center" },
                "children": [{ "tag": "text", "props": { "content": "A" } }]
              },
              {
                "tag": "tableheadcolumn",
                "props": { "align": "center" },
                "children": [{ "tag": "text", "props": { "content": "B" } }]
              }
            ]
          },
          {
            "tag": "tablebody",
            "children": [
              {
                "tag": "tablerow",
                "children": [
                  {
                    "tag": "tablecolumn",
                    "children": [{ "tag": "text", "props": { "content": "c" } }]
                  },
                  {
                    "tag": "tablecolumn",
                    "children": [{ "tag": "text", "props": { "content": "d" } }]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
]
